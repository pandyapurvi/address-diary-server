<h1><%= @address.street %></h1>

<ul>

  <li>streetNumber: <%= @address.streetNumber %></li>
  <li>street: <%= @address.street %></li>
  <li>suburb:<%= @address.suburb %></li>
  <li>postcode: <%= @address.postcode %></li>
  <li>state: <%= @address.state %></li>
  <li>unitNumber: <%= @address.unitNumber %></li>
  <li>streetType: <%= @address.streetType %></li>
  <li>propertyType: <%= @address.propertyType %></li>
</ul>

<div >
  <%= link_to 'Edit Address', edit_address_path(@address.id) %>
  <%= link_to 'Delete Address', address_path(@address.id), :method => 'delete', :data => {:confirm => 'Are you sure?'} %>
  </div>
